{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "@cencori/ai-sdk",
    "version": "0.2.1",
    "description": "Cencori AI SDK - The infrastructure layer for AI applications. Works with Vercel AI SDK, TanStack AI, and more.",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "keywords": [
        "ai",
        "cencori",
        "ai-sdk",
        "vercel",
        "tanstack",
        "provider",
        "streaming",
        "llm",
        "openai",
        "anthropic",
        "gemini",
        "infrastructure",
        "security",
        "observability"
    ],
    "author": "FohnAI",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cencori/cencori.git"
    },
    "homepage": "https://cencori.com",
    "bugs": {
        "url": "https://github.com/cencori/cencori/issues"
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "@ai-sdk/provider": ">=1.0.0"
    },
    "devDependencies": {
        "@ai-sdk/provider": "^3.0.0",
        "@ai-sdk/provider-utils": "^3.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    }
}